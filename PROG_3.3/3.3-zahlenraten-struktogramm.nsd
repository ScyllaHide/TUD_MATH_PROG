																<instruction text="&#34;vergleich = 1&#34;" comment="&#34;&#34;" color="ffffff" rotated="0"></instruction>
																<instruction text="&#34;Ausgabe: Fehler&#34;" comment="&#34;&#34;" color="ffffff" rotated="0"></instruction>
